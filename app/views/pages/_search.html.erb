<%= form_tag '/search', class: 'form-inline', id: 'search_form' do %>
  <i class="icon-search"></i>
  <%= text_field_tag :search, nil, placeholder: 'Search...', class: width, id: 'search', autocomplete: 'off' %>
  <%#= submit_tag 'Search' %>
<% end %>

